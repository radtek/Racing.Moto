@{
    ViewBag.Title = "竞技场";
}
<script src="/Scripts/jquery.signalR-2.2.1.js"></script>
<script src="/signalr/hubs"></script>
<script src="~/Scripts/jquery.easing.1.3.js"></script>
<script src="~/Scripts/jquery.floatingBg.js"></script>
<script src="~/Scripts/jquery.floating.js"></script>
<link href="~/Content/racing.css" rel="stylesheet" />

@Scripts.Render("~/Racing")

<div class="game-wrap">
    <div class="bg-top">
        <div class="shishi">
            <div class="shishi-1 lv-se" style="margin-left: 235px;">1</div>
            <div class="shishi-2 hong-se">2</div>
            <div class="shishi-3 zi-se">3</div>
            <div class="shishi-4 juhuang-se">4</div>
            <div class="shishi-5 danhong-se">5</div>
            <div class="shishi-6 shenlan-se">6</div>
            <div class="shishi-7 tianlan-se">7</div>
            <div class="shishi-8 lan-se">8</div>
            <div class="shishi-9 huang-se">9</div>
            <div class="shishi-10 he-se">10</div>
        </div>
    </div>
    <div class="saidao">
        <div class="time-run"><img id="num" /></div>
        @*start flag*@
        <div class="start-flag">
            <img src="/img/zhu-a.png" class="zhu-a" />
        </div>
        @*end flag*@
        <div class="end-flag">
            <div class="end"></div>
            <img src="/img/end-wang.png" class="end-wang" />
        </div>

        <div class="car-list">
            <img src="/img/car-10.png" class="car-10" />
            <img src="/img/car-9.png" class="car-9" />
            <img src="/img/car-8.png" class="car-8" />
            <img src="/img/car-7.png" class="car-7" />
            <img src="/img/car-6.png" class="car-6" />
            <img src="/img/car-5.png" class="car-5" />
            <img src="/img/car-4.png" class="car-4" />
            <img src="/img/car-3.png" class="car-3" />
            <img src="/img/car-2.png" class="car-2" />
            <img src="/img/car-1.png" class="car-1" />
        </div>
        @*start flag*@
        <div class="start-flag">
            <img src="/img/zhu-b.png" class="zhu-b" />
            <img src="/img/wang.png" class="wang" />
        </div>
        @*end flag*@
        <div class="end-flag saiqi hide">
            <img src="/img/saiqi.png">
        </div>
    </div>
</div>
