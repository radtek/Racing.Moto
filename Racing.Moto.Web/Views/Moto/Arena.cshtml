@{
    ViewBag.Title = "竞技场";
}
<script src="/Scripts/jquery.signalR-2.2.1.js"></script>
<script src="/signalr/hubs"></script>
<script src="~/Scripts/jquery.easing.1.3.js"></script>
<script src="~/Scripts/jquery.timer.js"></script>
<script src="~/Scripts/jquery.floatingBg.js"></script>
<script src="~/Scripts/jquery.floating.js"></script>
<link href="~/Content/racing.css" rel="stylesheet" />

@Scripts.Render("~/Racing")

<div class="game-wrap">
    <div class="bg-top">
        <div class="shishi" id="ranks">
            <div style="margin-left:184px; ">&nbsp;</div>
            <div class="shishi-1 color-1">1</div>
            <div class="shishi-2 color-2">2</div>
            <div class="shishi-3 color-3">3</div>
            <div class="shishi-4 color-4">4</div>
            <div class="shishi-5 color-5">5</div>
            <div class="shishi-6 color-6">6</div>
            <div class="shishi-7 color-7">7</div>
            <div class="shishi-8 color-8">8</div>
            <div class="shishi-9 color-9">9</div>
            <div class="shishi-10 color-10">10</div>
        </div>
    </div>
    <div class="saidao">
        <div class="time-run"><img id="countdown" /></div>
        @*start flag*@
        <div class="start-flag">
            <div class="start"></div>
            <img src="/img/zhu-a.png" class="zhu-a" />
        </div>
        @*end flag*@
        <div class="end-flag">
            <div class="end"></div>
            <img src="/img/zhu-a.png" class="zhu-a" />
            @*<div class="end"></div>
            <img src="/img/end-wang.png" class="end-wang" />*@
        </div>

        <div class="car-list">
            <img src="/img/moto-10.png" class="car-10" />
            <img src="/img/moto-9.png" class="car-9" />
            <img src="/img/moto-8.png" class="car-8" />
            <img src="/img/moto-7.png" class="car-7" />
            <img src="/img/moto-6.png" class="car-6" />
            <img src="/img/moto-5.png" class="car-5" />
            <img src="/img/moto-4.png" class="car-4" />
            <img src="/img/moto-3.png" class="car-3" />
            <img src="/img/moto-2.png" class="car-2" />
            <img src="/img/moto-1.png" class="car-1" />
        </div>
        @*start flag*@
        <div class="start-flag">
            <img src="/img/zhu-b.png" class="zhu-b" />
            <img src="/img/wang.png" class="wang" />
        </div>
        @*end flag*@
        <div class="end-flag">
            <img src="/img/zhu-b.png" class="zhu-b" />
            <img src="/img/wang.png" class="wang" />
        </div>
        <div class="saiqi hide">
            <img src="/img/saiqi.png">
        </div>
    </div>
</div>
