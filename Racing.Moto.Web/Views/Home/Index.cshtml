@{
    ViewBag.Title = "首页";
}<!--Script references. -->
<script src="/Scripts/jquery.signalR-2.2.1.js"></script>
<!--Reference the autogenerated SignalR hub script. -->
<script src="/signalr/hubs"></script>
@*<script src="~/Scripts/PK/Racing.js"></script>*@
<script src="~/Scripts/jquery.floatingBg.js"></script>
<link href="~/Content/racing.css" rel="stylesheet" />

@Scripts.Render("~/Racing")
<div>
    <h1>ASP.NET SignalR Sample</h1>

    <h2>实时推送</h2>
    <div id="stockTable"></div>

    <div class="board">
        <div id="roadBg" class="roadbed roadRun"  bg="/images/road.png"></div>
        <div class="road">
            <div class="left"></div>
            <div class="enemy"></div>
        </div>
        <span class="start"></span>
        <span class="kilo"></span>
        <div class="failbub">
            <span class="failtext"></span>
            <span class="retry"></span>
        </div>
    </div>
</div>